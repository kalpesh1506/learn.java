import java.util.Scanner;


public class Operators {
	
	public static void main(String[] args) {
		double n = Double.parseDouble(args[0]);
		double m = Double.parseDouble(args[1]);
		int operator = 0;
		System.out.println();
		operator = Integer.parseInt(args[2]);
		 switch(operator)
		 {
		 case 1:
			 System.out.println("Sum Operator: n+m = " + (n+m));
			 break;
		 case 2:
			 System.out.println("Subtraction Operator: n-m = " + (n-m));
			 break;
		 case 3:
			 System.out.println("Remainder Operator: n%m = " + (n%m));
			 break;
		 case 4:
			 System.out.println("Multiply Operator: n*m = " + (n*m));
			 break;
		 case 5:
			 System.out.println("Division Operator: n/m = " + (n/m));
			 break;
		 default:
			 System.out.println("Not a valid operator");
			 break;
		 }
	}
}
